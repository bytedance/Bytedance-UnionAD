    pod 'Google-Mobile-Ads-SDK'
    pod 'MLeaksFinder'
    pod 'MBProgressHUD'
    pod 'libwebp', '~> 0.5.1'
    
    target 'BUADDemo' do
      project 'BUADDemo.xcodeproj'

    end
    
end

