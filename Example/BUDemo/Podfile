abstract_target 'Demo' do
    pod 'AFNetworking', '~> 3.0', :subspecs => ['UIKit'], :inhibit_warnings => false
    pod 'FLEX', '~> 2.0', :inhibit_warnings => false
    pod 'KVOController'
